            <Setter Property="TextColor" Value="{DynamicResource DarkDetailColour}"/>
        </Style>
        <Style TargetType="{x:Type Label}" BasedOn="{StaticResource BaseLabelStyle}"/>

        <local:WasteProcessingToColourConverter x:Key="WasteProcessingToColourConverter"/>
        <local:WasteProcessingToAdjectiveConverter x:Key="WasteProcessingToAdjectiveConverter"/>
        <local:WasteProcessingToPathConverter x:Key="WasteProcessingToPathConverter"/>
        <local:SelectionConverter x:Key="SelectionConverter"/>
        <local:ImageSourceConverter x:Key="ImageSourceConverter"/>

    </Application.Resources>
</Application>